    "repo": "proto/wtools/atop/repo/entry/ExecUnrestricted"
  },
  "dependencies": {
    "wTools": "latest",
    "wrepobasic": "latest",
    "wprocess": "latest",
    "wgittools": "latest",
    "wCopyable": "latest",
    "wcommandsaggregator": "latest"
  },
  "devDependencies": {
    "wTesting": "latest",
    "eslint": "7.32.0"
  },
  "keywords": [
    "wTools"
