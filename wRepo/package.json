{
  "name": "wrepo",
  "version": "0.3.399",
  "description": "Utility to work with remote git repositories.",
  "author": "Kostiantyn Wandalen <wandalen@obox.systems>",
  "contributors": [
    "Kostiantyn Wandalen <wandalen@obox.systems>",
    "Dmytro Kryvoruchko <dm.vict.kr@gmail.com>"
  ],
  "license": "MIT",
  "main": "proto/node_modules/wrepo",
  "files": [
    "proto/wtools/atop/repo",
    "out",
    "proto/node_modules",
    "proto/node_modules/Tools",
    "proto/node_modules/wrepo"
  ],
  "scripts": {
    "test": "wtest .run 'proto/**' rapidity:-3",
    "docgen": "wdocgen proto"
