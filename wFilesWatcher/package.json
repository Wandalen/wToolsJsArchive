{
  "name": "wfileswatcher",
  "version": "0.1.65",
  "engine": "node >= 10.0.0",
  "enabled": 1,
  "description": "Aggregates file watching strategies and provides a single interface to them. The strategy can be a wrapper around a built-in feature or a separate library. Use the module to easily create watcher instances, pause/resume watchers, switch between watcher strategies and control created watchers using the watcher manager class.",
  "license": "MIT",
  "author": "Kostiantyn Wandalen <wandalen@obox.systems>",
  "contributors": [
    {
      "name": "Kostiantyn Wandalen",
      "email": "wandalen@obox.systems"
    },
    {
      "name": "Volodymyr M.",
      "email": "volodymyr.migdal@gmail.com"
    }
  ],
  "main": "proto/node_modules/wFilesWatcher",
  "files": [
    "proto/wtools/amid/l4_files/l7_watcher",
    "out",
    "proto/node_modules/Tools",
    "proto/node_modules/wFilesWatcher"
  ],
  "scripts": {
    "test": "wtest .run 'proto/**' rapidity:-1",
    "docgen": "wdocgen proto",
    "watches.limit.increase": "node proto/wtools/amid/l4_files/l7_watcher/step/WatchesLimit.s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wandalen/wFilesWatcher.git"
  },
  "bugs": {
    "url": "https://github.com/Wandalen/wFilesWatcher/issues"
  },
  "dependencies": {
    "wTools": "latest",
    "wProto": "latest",
    "winstancing": "latest",
    "wFiles": "latest",
    "wEventHandler": "latest",
    "wCopyable": "latest",
    "wConsequence": "latest",
    "wcloner": "latest",
    "fb-watchman": "2.0.1"
  },
  "devDependencies": {
    "wTesting": "latest",
    "eslint": "7.32.0"
  },
  "keywords": [
    "wTools",
    "wFilesWatcher",
    "fileswatcher"
  ]
}
