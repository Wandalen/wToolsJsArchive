  "contributors": [
    {
      "name": "Kostiantyn Wandalen",
      "email": "wandalen@obox.systems"
    }
  ],
  "license": "MIT",
  "main": "proto/node_modules/wcloner",
  "files": [
    "proto/wtools/abase/l5",
